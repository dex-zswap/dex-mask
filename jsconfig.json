{
  "target": "es6",
  "module": "commonjs",
  "compilerOptions": {
    "baseUrl": "",
    "paths": {
      "@app/*": [
        "./app/*"
      ],
      "@shared/*": [
        "./shared/*"
      ],
      "@view/*": [
        "./view/*"
      ],
      "@c/*": [
        "./view/components/*"
      ],
      "@pages/*": [
        "./view/pages/*"
      ],
      "@reducer/*": [
        "./view/reducer/*"
      ],
      "@selectors/*": [
        "./view/selectors/*"
      ],
      "@wallet-provider/*": [
        "./wallet-provider/*"
      ]
    }
  },
  "include": [
    "view/**/*.js"
  ]
}